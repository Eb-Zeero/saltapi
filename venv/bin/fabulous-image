#!/home/sifiso/saao-work/saltapi/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'fabulous==0.3.0','console_scripts','fabulous-image'
__requires__ = 'fabulous==0.3.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('fabulous==0.3.0', 'console_scripts', 'fabulous-image')()
    )
